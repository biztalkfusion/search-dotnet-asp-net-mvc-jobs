<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title</title>

    <!-- Stylesheets -->
    <link href='http://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic|Roboto+Condensed:400,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="~/content/bootstrap.css">
    <link rel="stylesheet" href="~/content/font-awesome.min.css">
    <link rel="stylesheet" href="~/content/flexslider.css">
    <link rel="stylesheet" href="~/content/style.css">
    <link rel="stylesheet" href="~/content/responsive.css">

    <!-- Attach CSS for Joyride which is the step through tutorial tool -->
    <link href="~/Content/joyride-2.1.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.joyride-2.1.js"></script>

    <!--[if IE 9]>
            <script src="js/media.match.min.js"></script>
    <![endif]-->
</head>
<body>
    <div id="wrapper">
        <header id="header">
            <div class="container header-wrapper">
                <div class="row">
            
                </div> <!-- end .header-wrapper .row -->
            </div> <!-- end .header-wrapper .container -->
            <div id="mobile-menu-container" class="container">
                <div class="login-register"></div>
                <div class="menu"></div>
            </div>

            <div class="header-nav">
                <div class="container">
                    <!-- Logo -->
                    <div class="logo-wrapper">
                        <div class="logo">
                            <a href="/"><img src="~/images/EDI Insights1.jpg" alt=""></a>
                        </div>
                    </div> <!-- end .logo-wrapper -->
                    <h1 id="available-jobs-label"></small></h1>

                    <ul class="nav">
                        <li><a href="/">Home</a></li>
                        <li><a href="/">Documents</a></li>
                        <li><a href="@Url.Action("LogOff", "Account")">Logout</a></li>
                    </ul>
                </div> <!-- end .header-nav .container -->
            </div>  <!-- end .header-nav -->
        </header>

        @RenderBody()

        <footer id="footer">
            <div class="container">
                <div class="row">
                    <div class="col-sm-4 col-md-5">
                        <div class="widget">
                            <div class="widget-content">
                                <img class="logo" src="~/images/EDI Insights1.jpg" alt="">
                                <p>This is a demo application based on data from the EDI files on FTP location. These files are not processed for live data. </p>
                            </div>
                        </div>
                    </div>

                    <div class="col-sm-3 col-md-3">
                        <div class="widget">
                            <h5 class="widget-title">Follow Us</h5>

                            <div class="widget-content">
                                <ul class="footer-links">
                                    <li><a href="https://Kuebix.com/blog/">Blog</a></li>
                                    <li><a href="https://twitter.com/search?src=typd&q=%23kuebix">Twitter</a></li>
                                
                                </ul>
                            </div>
                        </div>
                    </div>

                    
                </div>
            </div>

            <div class="copyright">
                <div class="container">
                    <p>&copy; Copyright 2017 <a href="http://www.BizTlkFusion.com">Ingenious Cloud Solutions All Rights Reserved</p>
                </div>
            </div>
        </footer> <!-- end #footer -->

    </div> <!-- end #wrapper -->
    <!-- Scripts -->
    <script>window.jQuery || document.write('<script src="~/scripts/jquery-1.11.0.min.js"><\/script>')</script>
    <script src="~/scripts/jquery.ba-outside-events.min.js"></script>
    <script src="~/scripts/jquery.responsive-tabs.js"></script>
    <script src="~/scripts/jquery.flexslider-min.js"></script>
    <script src="~/scripts/jquery.fitvids.js"></script>
    <script src="~/scripts/jquery.inview.min.js"></script>
    <script src="~/scripts/script.js"></script>

</body>
</html>
